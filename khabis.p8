--miveh (fruit)

miveh = {
		x=64,
		y=64,
		spr=2,
		max=15,

		draw=function()
				spr(miveh.spr,miveh.x+vm
				,miveh.y+vm/4)
				
				
			--vibarte	
		if vc >0 then
 		vm+=vn
 		if abs(vm) > 1 then
 				vn = vn * -1
 		end
 		vc-=1
		end
		
		
		
		end, -- draw
		
		random=function()
		  
				miveh.x = rnd(80)+32
				miveh.y = rnd(80)+32
				if collision(mario.x, mario.y, miveh.x, miveh.y) then
				miveh.x = (mario.x + 100)%
				 128+16
				end
				
				--bomb or miveh(fruit), that's the question
				if rnd(100) > 50 then 
				 	bomb.activate(miveh.x,
					 miveh.y)
					return
				end
				
				miveh.spr=16+ rnd(miveh.max)
		vc=10
		end, -- random
	
}


--vibrate effect
vm = 1
vn = 0.5
vc = 4

