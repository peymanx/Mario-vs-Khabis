version 16
__lua__
--mario vs khabis
--by peyman-x
function _init()
		music(1)
		effect = 0
		ec = 11
		mx = 0
		my = 0
		game_over = false
		start_screen = true
		shake =0
end


function _update()

		if start_screen then
		  if btnp(❎) then
		 		 start_screen=false
		 		 music(0)
		 		 mario.score =0
		 		 khabis.score =0
		 		 miveh.random()
		  
		  end	
				return
		end
		
		
		if game_over then	
		  if btnp(❎) then
		      _init()
		  end
				return
		end
		

		mario.update()
		khabis.update(miveh)
		mario.collision(miveh)
		khabis.collision(miveh)

end



function _draw()

	shaking()
	
	if game_over then	
			gameover()
			return
 end
 
 if start_screen then	
			startscreen()	
			return
 end

 

		cls()
		
		--menu
		scores()
		map(17,0)


		khabis.draw()
		
		
		--bomb! or miveh(fruit)
		if bomb.active then
				bomb.draw()
		else
		  miveh.draw()				
		end
		
		mario.draw()
		
		
		--effect when eating miveh (fruit)
		if effect >0 then
					circ(mx+4,my+4, effect, ec)
					circ(mx+4,my+4, effect/2, 10)
					circ(mx+4,my+4, effect/3, 7)
					effect-=1		
		end
		


end




-->8
--mario
speed=1
m=0
mario = {
		x=64,
		y=64,
		spr=2,
		score = 0,

		draw=function()
				spr(mario.spr+m,mario.x,mario.y)
		
		end,
		
		update=function()
		m+=0.1
		m=m%3
				x = mario.x
				y = mario.y
				if btn(⬅️) then
							mario.x-=speed
							mario.spr=4				
				end
				if btn(➡️) then
							mario.x+=speed	
							mario.spr=1			
				end
				
				if btn(⬆️) then
							mario.y-=speed				
				end
				if btn(⬇️) then
							mario.y+=speed				
				end
				
				
				if collision(mario.x, mario.y,
				 khabis.x, khabis.y) then
									mario.x = x
									mario.y = y
				end
				
			--touch bomb!	
			if bomb.active and collision(bomb.x, bomb.y,
				 mario.x, mario.y) then
				 shake = 10
				 effect=100
				 game_over = true
				 sfx(8)
								
				end
				

		
		end,
		
		collision=function(m)
				if collision(mario.x, mario.y, m.x, m.y) then
						mario.score+=1
						mx = m.x
						my = m.y
						m.random()
						sfx(0)
						effect = 10
						ec = 8
						
				end
		
		
		end,


}


function collision(a,b,x,y)

		if (a <= x and a+8 >=x or 
					x <= a and x+8 >=a) and
					 (b <= y and b+8 >=y or 
					y <= b and y+8 >=b) then
					return true
			end
			
			return false
					

end
