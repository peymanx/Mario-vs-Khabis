--khabis (bad guy)

k=0  --open/close khabis mouth helper
timer_to_revive = 0

khabis = {
		x=100,
		y=100,
		spr=32,
		dspr=39, -- dead sprite
		score = 0,
		alive = true,

		draw=function()
		if khabis.alive then
				spr(khabis.spr+k,khabis.x,khabis.y)
		else
				spr(khabis.dspr+k,khabis.x,khabis.y)
				print(-5, khabis.x, 
				  khabis.y-bs)
				
		end
		
		end,
		
		update=function(m)
		
		
		
 		if not khabis.alive then
 					 k+=0.05
 					 k=k%3
 					 timer_to_revive-=1
 					 
 					 if timer_to_revive < 0 then
 					 khabis.alive = true
 					 
 					 end
 		
 		
 		
 		
   					return
  		end
  		
  		--open/close khabis mouth
  		k+=0.2		
  		k=k%3
  			 
			 x = khabis.x
			 y = khabis.y
		
				if khabis.x < m.x then
						khabis.x+=speed/1.5
								khabis.spr=32
				
				end
				
				if khabis.x > m.x then
						khabis.x-=speed/1.5
						
						khabis.spr=35
						
				end



				if khabis.y > m.y then
						khabis.y-=speed/1.5
				end
				
			 if khabis.y < m.y then
						khabis.y+=speed/1.5
				end
				
				
				if collision(mario.x, mario.y,
				 khabis.x, khabis.y) then
									khabis.x = x +1
									khabis.y = y
				end
				
				--touch bomb!
				if bomb.active and collision(bomb.x, bomb.y,
				 khabis.x, khabis.y) then
				 khabis.score-=5
				 if (khabis.score<0) khabis.score=0
				 
				 khabis.alive = false
				 timer_to_revive = 70
				 shake = 10
				 bs=12 -- bomb sprite to exploedd version
				 sfx(4)
								
				end
	



				

		
		end,
		
		collision=function(m)
				if collision(khabis.x, khabis.y, m.x, m.y) then
						khabis.score+=1
						mx = m.x
						my = m.y
						m.random()
						sfx(0)
						effect = 10
						ec = 11
						
						
				end
		
		
		end,


}


