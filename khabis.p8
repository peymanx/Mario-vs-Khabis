--menus
function scores()
		print("mario:"..mario.score,
	   	2,2,7)
		print("khabis:"..khabis.score,
		   85,2,7)
end

blinking=1
function startscreen()
		cls(0)
		
	

		print("mario vs khabis",
		(blinking*10)%128,18,7)
		 
		map(0,0)
		print("try to get fruits:",15,28,7)
		spr(16, 15, 36,12, 1)
		
		bomb.x=15 ; bomb.y=54
  bomb.active=false
		print("and avoid bombs!",15,46,7)
		bomb.draw()
		if (bs > 11) bs=1
		
		
		print("press ❎ to start",31,100,
		blinking%7+1)

		
	
	 mario.x=15 
	 mario.y=70
	 khabis.x=100
	 khabis.y=70
		mario.draw()
		
		khabis.update(miveh)
		khabis.draw()
		
		blinking+=0.1
		blinking=blinking%1000
		
end

function gameover()
		cls(8)
		print("game over",45,64,7)
			
end
