--shake
--when touch the bomb
function shaking()

		negative = 1
		
		if rnd(100)>50 then
				negative = -1
		end
  if shake < 0 then
  
    camera(0,0)
    return
  
  end

		camera(rnd(shake)* negative,
		 rnd(shake)* negative)
		shake-=1

end

