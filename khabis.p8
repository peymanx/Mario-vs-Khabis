--bomb!
bs=0
bomb = {
		x=64,
		y=64,
		spr=48,
		max=15,
		active = false,
		
		activate=function(x,y)
					if (bomb.active) return
					
					bomb.x = x
					bomb.y = y
					
   		bomb.active = true
   		bs = 0
   		
   		sfx(3)
   		
		
		
		end,

		draw=function()
				spr(bomb.spr+bs,bomb.x	,bomb.y)
				bs+=0.2
				if bs > 13 then -- explode
						bomb.active = false
						effect=16
						ec=5
						mx=bomb.x
						my=bomb.y
						miveh.random()
						sfx(4)
				
				end
				
			
		end, -- draw
		

}

